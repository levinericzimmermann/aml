<!DOCTYPE patchbay>
<patchbay version="0.4.5" name="patchbay_b">
 <output-sockets>
  <socket client="pyo" exclusive="off" type="jack-midi" name="pyo 1">
   <plug>output</plug>
  </socket>
  <socket client="system" exclusive="off" type="jack-midi" name="system - midi -keyboard">
   <plug>midi_capture_1</plug>
  </socket>
  <socket client="system" exclusive="off" type="jack-midi" name="system - midi - korg">
   <plug>midi_capture_2</plug>
  </socket>
  <socket client="Pianoteq" exclusive="off" type="jack-audio" name="Pianoteq 1">
   <plug>out_1</plug>
   <plug>out_2</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket client="Pianoteq" exclusive="off" type="jack-midi" name="Pianoteq 1">
   <plug>midi_in</plug>
  </socket>
  <socket client="pyo" exclusive="off" type="jack-audio" name="pyo 1">
   <plug>input_4</plug>
  </socket>
  <socket client="pyo" exclusive="off" type="jack-midi" name="pyo-midi-input">
   <plug>input</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable input="Pianoteq 1" output="pyo 1" type="jack-midi"/>
  <cable input="pyo-midi-input" output="system - midi -keyboard" type="jack-midi"/>
  <cable input="Pianoteq 1" output="system - midi -keyboard" type="jack-midi"/>
  <cable input="pyo 1" output="Pianoteq 1" type="jack-audio"/>
  <cable input="pyo-midi-input" output="system - midi - korg" type="jack-midi"/>
 </cables>
</patchbay>

